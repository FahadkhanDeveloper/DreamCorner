{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/app.js"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/listings",
        "permanent": false
      }
    ]
  }